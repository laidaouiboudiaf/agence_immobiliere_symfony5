<div class="card mb-4">
    <div class="card-body">
        <h5 class="card-title">
            <a href="{{ path('owner.show',{id:property.id, slug: property.slug}) }}">{{ property.title }}</a>

        </h5>

        <p class="card-text"> {{ property.city }} ({{ property.postalCode }})</p>
        <div class="text-primary"
             style="font-weight: bold;size: 2rem"> {{ property.price|number_format(0,'', ' ') }} $
        </div>
    </div>


</div>